{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "5.2.1"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": "=18.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "scripts": {
    "docker:unoptimized:build": "docker-compose -f Dockerfiles/unoptimized/docker-compose.yaml build",
    "docker:unoptimized:run": "docker-compose -f Dockerfiles/unoptimized/docker-compose.yaml up",
    "docker:unoptimized:down": "docker-compose -f Dockerfiles/unoptimized/docker-compose.yaml down -v"
  },
  "packageManager": "yarn@3.5.1"
}
